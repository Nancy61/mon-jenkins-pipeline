pipeline {
    agent any 
    
    stages{
        stage('Build') {
            steps{
                sh 'echo "Building feature1 branch"'
            }
        }
        stage('Test') {
            steps{
                sh 'echo "Testing feature1 branch"'
            }
        }
        stage('Deploy') {
            steps{
                sh 'echo "Deploying on feature1 branch"'
            }
        }
    }
}